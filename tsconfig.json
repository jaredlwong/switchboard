{
  "compilerOptions": {
    "target": "es6",
    "lib": ["dom", "dom.iterable", "esnext"],
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "dist",
    "strict": true,
    "esModuleInterop": true,
    "jsx": "react",
    "allowSyntheticDefaultImports": true,
    "noErrorTruncation": true,
    "noImplicitAny": false,
    "forceConsistentCasingInFileNames": false
  },
  "include": ["src"]
}
